___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const copyFromDataLayer = require('copyFromDataLayer');
const sendPixel = require('sendPixel');
const JSON = require('JSON');
const encodeUriComponent = require('encodeUriComponent');
const toBase64 = require('toBase64');
const makeInteger = require('makeInteger');
const event = copyFromDataLayer('event');

if (data.merchant_id == undefined || data.merchant_id <= 0) {
  log('Invalid Merchant ID!');
  data.gtmOnFailure();
  return;
}
if (data.api_key == undefined || data.api_key.trim().length === 0) {
  log('Invalid API Key!');
  data.gtmOnFailure();
  return;
}

const merchant_id = makeInteger(data.merchant_id);
const api_key = data.api_key.trim();

const required_attributes = ['item_id', 'store_code', 'availability', 'price',
                             'language', 'target_country'];
const optional_attributes = ['pickup_method', 'pickup_sla'];

let inventory = {'merchant_id': merchant_id};

let valid = false;
if (data.read_from_data_layer) {
  valid = fetchAttributesFromDataLayer();
} else {
  valid = fetchAttributesFromInput();
}
if (!valid) {
  data.gtmOnFailure();
  return;
}

log('inventory pixel generated: ', inventory);

const pixelUrl =
      'https://wsri.googleapis.com/v1alpha/' +
      encodeUriComponent(toBase64(JSON.stringify(inventory))) +
    '?key=' + encodeUriComponent(api_key);

sendPixel(pixelUrl, data.gtmOnSuccess(), data.gtmOnFailure());

function fetchAttributesFromDataLayer() {
  for (const attr of required_attributes) {
    const value = copyFromDataLayer(attr);
    if (value != undefined && value != null && value.trim().length > 0) {
      inventory[attr] = value.trim();
    } else {
      log('Missing attribute ', attr, ' in data layer.');
      return false;
    }
  }
  for (const attr of optional_attributes) {
    const value = copyFromDataLayer(attr);
    if (value != undefined && value != null && value.trim().length > 0) {
      inventory[attr] = value.trim();
    }
  }
  return true;
}

function fetchAttributesFromInput() {
  for (const attr of required_attributes) {
    const value = data[attr];
    if (value != undefined && value != null && value.trim().length > 0) {
      inventory[attr] = value.trim();
    } else {
      log('Missing value for input field ', attr);
      return false;
    }
  }
  for (const attr of optional_attributes) {
    const value = data[attr];
    if (value != undefined && value != null && value.trim().length > 0) {
      inventory[attr] = value.trim();
    }
  }
  return true;
}


